{
  "$schema": "http://json-schema.org/schema",
  "$id":"feature",
  "title": "Store Feature Schema",
  "type": "object",
  "properties":{
    "name":{
      "type":"string",
      "description": "name of all template",
      "default": "new-feature",
      "x-prompt": "Choose a name for your feature"
    },
    "path":{
      "type":"string",
      "default": "src/app/data/store/",
      "description": "Path to create all templates",
      "x-prompt": "Select a path to create your feature"
    }
  }
}
