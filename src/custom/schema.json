{
  "$schema": "http://json-schema.org/schema",
  "$id":"feature",
  "title": "Feature Schema",
  "type": "object",
  "properties":{
    "name":{
      "type":"string",
      "description": "name of all template",
      "default": "app",
      "x-prompt": "Choose a name for your feature"
    },
    "appRoot":{
      "type":"string"
    },
    "path":{
      "type":"string",
      "default": "src/app/data/store/settings/",
      "description": "Path to create all templates",
      "x-prompt": "Select a path to create your feature"
    }
  }
}
